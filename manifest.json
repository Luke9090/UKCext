{
  "name": "UKC like/dislike hider",
  "version": "1.1",
  "description": "Hides the like/dislike buttons on UKClimbing Forum threads",
  "manifest_version": 2,
  "icons": { "16": "icons/icon16.png",
           "32": "icons/icon32.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" },
  "content_scripts": [
    {
        "matches": ["*://www.ukclimbing.com/forums/*","*://www.ukhillwalking.com/forums/*"],
        "js": ["/content_script.js"],
        "run_at": "document_end"
    }
  ]
}
